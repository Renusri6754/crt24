public class Main {
    public static void main(String[] args) {
        int [] a={3,2,1,2,2,1,3,1,5,5,4,1};
        int n=a.length;
        int count=0;
        for(int i=0;i<n;i++)
        {
            if (a[i]==0)
            {
                count++;

            }
        }
        System.out.println(0+""+count);


    }
}
